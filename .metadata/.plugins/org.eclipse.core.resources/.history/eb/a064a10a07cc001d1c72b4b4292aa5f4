package a;

public class Pontos {
    private double x1, x2, y1, y2;

    public Pontos(double x1, double y1, double x2, double y2) {
        this.x1 = x1;
        this.y1 = y1;
        this.x2 = x2;
        this.y2 = y2;
    }

    public double getDistanciaEuclidiana() {
        double dx = x1 - x2;
        double dy = y1 - y2;
        return DistanciaEucli.distanciaEuclidiana(dx, dy);
    }
}